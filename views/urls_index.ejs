<!DOCTYPE html>
<html lang="en">
<body>
    <header>
        <% include ./partials/_header %>
    </header>
    <!-- Write EJS code here to display shortened li nks -->
    <% for (key in urls) { %>
    <ul>
      <%= key %> ->
      <a href="<%= urls[key] %>"><%= urls[key] %></a>
      <a href="/urls/<%= key %>">Edit</a>

      <form method="POST" action="/urls/<%= key %>?_method=DELETE">
      <button type="submit">Delete</button>
      </form>
    </ul>

    <% } %>
</body>
</html>